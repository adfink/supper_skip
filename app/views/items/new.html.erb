<h1> Create Your New Item </h1>

<%= form_tag restaurant_items_path(@restaurant), method: :post do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>

  <%= label_tag :description %>
  <%= text_field_tag :description %><br>

  <%= label_tag :price %>
  <%= text_field_tag :price %><br>

  <%= submit_tag %>
<% end %>
