<h1> Log In </h1>

<%= form_tag login_path do %>
  <%= label_tag :email_address %>
  <%= text_field_tag :email_address %>
  <br>
  <%= label_tag :password %>
  <%= password_field_tag :password%>
  <br>
  <%= submit_tag value="Log In" %>
<% end %>

<p>New user? <%= link_to "Sign up now!", new_user_path %></p>

